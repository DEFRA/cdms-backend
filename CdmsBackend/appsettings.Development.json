{
    "OTEL_EXPORTER_OTLP_ENDPOINT": "http://grafana.localtest.me:4317",
    "OTEL_SERVICE_NAME": "Cdms",
    "Mongo": {
        "DatabaseUri": "mongodb://127.0.0.1:29017?retryWrites=false",
        "DatabaseName": "cdms-backend"
    },
    "ApiOptions": {
        "EnableManagement": true
    },
    "SensitiveDataOptions": {
        "Include": true
    },
    "BlobServiceOptions": {
        "DmpBlobContainer": "dmp-data-1001",
        "DmpBlobUri": "https://snddmpinfdl1001.blob.core.windows.net"
    },
    "DetailedErrors": true,
    "AllowedHosts": "*",
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Information",
                "System": "Information"
            }
        },
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithThreadId"
        ],
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "formatter": "Elastic.CommonSchema.Serilog.EcsTextFormatter, Elastic.CommonSchema.Serilog"
                }
            }
        ]
    }
}
